{
  "name": "vehicle-parking-lot-system",
  "version": "1.0.0",
  "description": "\"It is all about vehicle parking lot system\"",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon dist/app.js",
    "compile": "tsc",
    "dev-watch": "tsx watch src/app.ts",
    "init-migration": "sequelize-cli init",
    "create-migration": "sequelize-cli migration:generate --name create-parking-session",
    "migrate": "sequelize-cli db:migrate",
    "migration-status": "sequelize-cli db:migrate:status",
    "undo-most-recent-migration": "sequelize-cli db:migrate:undo",
    "undo-all-migration": "sequelize-cli db:migrate:undo:all",
    "revert-back-to-specific-migration": "sequelize-cli db:migrate:undo:all --to 20260218144640-create-parking-slot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chukwutem-emi/Vehicle-Parking-Lot-System.git"
  },
  "author": "Chukwutem Stephen Emi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chukwutem-emi/Vehicle-Parking-Lot-System/issues"
  },
  "homepage": "https://github.com/chukwutem-emi/Vehicle-Parking-Lot-System#readme",
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.3",
    "@types/sinon": "^21.0.0",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "sinon": "^21.0.1",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express-validator": "^2.20.33",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.2",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "mysql2": "^3.17.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0"
  }
}
